{
  "name": "elm-boilerplate",
  "version": "0.0.1",
  "description": "Elm project boilerplate",
  "scripts": {
    "start": "npm run build-elm && npm run start-server",
    "build-elm": "elm make src/Main.elm --output dist/example.js",
    "start-server": "./node_modules/local-web-server/bin/cli.js -s index.html"
  },
  "author": "Elie Genard",
  "homepage": "https://github.com/elaye/elm-boilerplate",
  "devDependencies": {
    "elm-webpack-loader": "^2.0.0",
    "local-web-server": "^1.1.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
}
